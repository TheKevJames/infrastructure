{
  "checks": {
    "memory": {
      "type": "standard",
      "command": "/opt/sensu/embedded/bin/check-memory.rb -w 80 -c 90",
      "subscribers": [ "default" ],
      "interval": 30
    },
    "swap_percent": {
      "type": "standard",
      "command": "/opt/sensu/embedded/bin/check-swap-percent.rb -w 80 -c 90",
      "subscribers": [ "default" ],
      "interval": 30
    },
    "memory_metric": {
      "type": "metric",
      "command": "/bin/bash -c '/opt/sensu/embedded/bin/metrics-memory.rb -s system.${HOSTNAME//./_}.memory'",
      "subscribers": [ "default" ],
      "interval": 30,
      "output_type": "graphite",
      "handlers": [ "graphite" ]
    },
    "memory_metric_percent": {
      "type": "metric",
      "command": "/bin/bash -c '/opt/sensu/embedded/bin/metrics-memory-percent.rb -s system.${HOSTNAME//./_}.memory_percent'",
      "subscribers": [ "default" ],
      "interval": 30,
      "output_type": "graphite",
      "handlers": [ "graphite" ]
    }
  }
}
