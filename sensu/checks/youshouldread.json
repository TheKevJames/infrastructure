{
  "checks": {
    "youshouldread_server_http": {
      "type": "standard",
      "command": "/opt/sensu/embedded/bin/check-http.rb -u http://localhost:28004/ping -q 'ok'",
      "subscribers": [ "youshouldread" ],
      "interval": 30
    },
    "youshouldread_web_http": {
      "type": "standard",
      "command": "/opt/sensu/embedded/bin/check-http.rb -u http://localhost:28003/ping -q 'ok'",
      "subscribers": [ "youshouldread" ],
      "interval": 30
    }
  }
}
