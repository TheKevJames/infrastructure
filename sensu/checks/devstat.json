{
  "checks": {
    "devstat_server_http": {
      "type": "standard",
      "command": "/opt/sensu/embedded/bin/check-http.rb -u http://localhost:28002/ping -q 'ok'",
      "subscribers": [ "devstat" ],
      "interval": 30
    },
    "devstat_web_http": {
      "type": "standard",
      "command": "/opt/sensu/embedded/bin/check-http.rb -u http://localhost:28001/ping -q 'ok'",
      "subscribers": [ "devstat" ],
      "interval": 30
    }
  }
}
